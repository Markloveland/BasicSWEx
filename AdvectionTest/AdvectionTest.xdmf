<?xml version="1.0"?>
<!DOCTYPE Xdmf SYSTEM "Xdmf.dtd" []>
<Xdmf Version="3.0" xmlns:xi="http://www.w3.org/2001/XInclude">
  <Domain>
    <Grid Name="mesh" GridType="Uniform">
      <Topology TopologyType="Quadrilateral" NumberOfElements="4" NodesPerElement="4">
        <DataItem Dimensions="4 4" NumberType="Int" Format="HDF">AdvectionTest.h5:/Mesh/mesh/topology</DataItem>
      </Topology>
      <Geometry GeometryType="XY">
        <DataItem Dimensions="9 2" Format="HDF">AdvectionTest.h5:/Mesh/mesh/geometry</DataItem>
      </Geometry>
    </Grid>
    <Grid Name="WSE(m)" GridType="Collection" CollectionType="Temporal">
      <Grid Name="WSE(m)" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="0" />
        <Attribute Name="WSE(m)" AttributeType="Scalar" Center="Node">
          <DataItem Dimensions="9 1" Format="HDF">AdvectionTest.h5:/Function/WSE(m)/0</DataItem>
        </Attribute>
      </Grid>
      <Grid Name="WSE(m)" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="60" />
        <Attribute Name="WSE(m)" AttributeType="Scalar" Center="Node">
          <DataItem Dimensions="9 1" Format="HDF">AdvectionTest.h5:/Function/WSE(m)/60</DataItem>
        </Attribute>
      </Grid>
    </Grid>
    <Grid Name="depth averaged velocity (m/s)" GridType="Collection" CollectionType="Temporal">
      <Grid Name="depth averaged velocity (m/s)" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="0" />
        <Attribute Name="depth averaged velocity (m/s)" AttributeType="Vector" Center="Node">
          <DataItem Dimensions="9 3" Format="HDF">AdvectionTest.h5:/Function/depth averaged velocity (m/s)/0</DataItem>
        </Attribute>
      </Grid>
      <Grid Name="depth averaged velocity (m/s)" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="60" />
        <Attribute Name="depth averaged velocity (m/s)" AttributeType="Vector" Center="Node">
          <DataItem Dimensions="9 3" Format="HDF">AdvectionTest.h5:/Function/depth averaged velocity (m/s)/60</DataItem>
        </Attribute>
      </Grid>
    </Grid>
  </Domain>
</Xdmf>
